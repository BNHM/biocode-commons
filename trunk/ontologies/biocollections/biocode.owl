        <rdfs:subClassOf rdf:resource="&obo;BFO_0000023"/>
        <obo:IAO_0000115 xml:lang="en">A role that inheres in some material entity as a result of the output of some successful material sampling process.</obo:IAO_0000115>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/biocode/biocode_0000027 -->

