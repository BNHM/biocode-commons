        <obo:IAO_0000115 xml:lang="en">A museum collection that has as part a organismal museum collection entity.</obo:IAO_0000115>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/biocode/biocode_0000033 -->

